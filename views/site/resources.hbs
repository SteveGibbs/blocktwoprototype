{{> plain-header}}
{{> secondary-site-header}}

<main class="mdl-layout__content">
    <div class="page-content">
        <div class="mdl-grid">

            <div class="mdl-cell mdl-cell--6-col">
                <div class="about-rectangle">
                    <div class="p15">
                        <h2>Site Resources</h2>
                        <div class="inline">
                            {{#each user_item}}
                                <p class="w150">{{first_name}} {{last_name}}</p>
                            {{/each}}
                            </div>
                        <br>
                        <div class="inline">
                            {{#each user_item}}
                                <br>
                                <p class="w150">{{rank}}</p>
                            {{/each}}
                        </div>
                        <br>
                        <div class="inline">
                            {{#each user_item}}
                                <div class="w150">
                                    <div class="image-cropper">
                                        <img src="{{imagePath}}" alt="" class="portrait">
                                    </div>
                                </div>

                            {{/each}}
                        </div>

                        </div>


                    </div>
                </div>
            </div>
            <div class="mdl-cell mdl-cell--1-col"></div>
            <div class="mdl-cell mdl-cell--5-col"></div>



        </div>
    <!-- Your content goes here -->
    </div>
</main>

<!-- this final div closes off the div started in the header.hbs -->
</div>

